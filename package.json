{
  "name": "magpie-bookmark",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Andre Ortiz <hello@andreortiz.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "copyFavicon": "cp -v favicon.ico ./dist/favicon.ico && echo ok",
    "copyManifest": "cp -v manifest.json ./dist/manifest.json && echo ok",
    "copyIndex": "cp -v ./templates/index.html ./dist/index.html && echo ok",
    "copyPopup": "cp -v ./templates/popup.html ./dist/popup.html && echo ok",
    "copyBundle": "npm run copyManifest && npm run copyIndex && npm run copyPopup && npm run copyPopupJS && npm run copyFavicon",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules && copyBundle"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0-beta.2",
    "faker": "^4.1.0",
    "firebase": "^4.7.0",
    "underscore": "^1.8.3",
    "vue": "^2.4.4",
    "vue-ls": "^2.3.3",
    "vuefire": "^1.4.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
